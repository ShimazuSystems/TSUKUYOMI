  "classification_handling_framework": {
    "classification_levels": {
      "UNCLASSIFIED": {
        "markers": ["UNCLASSIFIED", "U", "UNCLAS"],
        "handling": "standard",
        "distribution": "unrestricted"
      },
      "CONFIDENTIAL": {
        "markers": ["CONFIDENTIAL", "C", "CONFIDENTIAL//"],

    "handling_instructions": {
      "NOFORN": {
        "markers": ["NOFORN", "NF"],
        "restriction": "no_foreign_nationals",
        "enforcement": "strict"
      },
      "REL_TO": {
        "markers": ["REL TO", "REL"],
        "restriction": "releasable_to_specified",
        "parameters": "country_list"
      },
      "EYES_ONLY": {
        "markers": ["EYES ONLY", "EO"],
        "restriction": "named_recipients_only",
        "enforcement": "absolute"
      },
      "ORCON": {
        "markers": ["ORCON", "OC"],
        "restriction": "originator_controlled",
        "requirement": "originator_approval"
      },
      "LIMDIS": {
        "markers": ["LIMDIS", "LD"],
        "restriction": "limited_distribution",
        "scope": "defined_list"
      }
    },
    "compartmentalization": {
      "SCI": {
        "markers": ["SCI", "SENSITIVE COMPARTMENTED INFORMATION"],
        "handling": "compartmented",
        "access": "read_on_required"
      },
      "SAP": {
        "markers": ["SAP", "SPECIAL ACCESS PROGRAM"],
        "handling": "special_access",
        "access": "explicit_authorization"
      }
    }
  },
  "source_evaluation_framework": {
    "reliability_assessment": {
      "A": {
        "description": "Completely reliable",
        "criteria": ["established track record", "direct access", "consistent accuracy", "no known deception"],
        "confidence_factor": 0.95
      },
      "B": {
        "description": "Usually reliable",
        "criteria": ["good track record", "generally accurate", "occasional minor errors", "trusted source"],
        "confidence_factor": 0.80
      },
      "C": {
        "description": "Fairly reliable",
        "criteria": ["mixed track record", "usually accurate", "some errors", "reasonable access"],
        "confidence_factor": 0.65
      },
      "D": {
        "description": "Not usually reliable",
        "criteria": ["poor track record", "often inaccurate", "limited access", "questionable motivation"],
        "confidence_factor": 0.40
      },
      "E": {
        "description": "Unreliable",
        "criteria": ["consistently inaccurate", "known fabrication", "hostile source", "deceptive history"],
        "confidence_factor": 0.20
      },
      "F": {
        "description": "Reliability cannot be judged",
        "criteria": ["new source", "no track record", "unknown access", "insufficient history"],
        "confidence_factor": 0.50
      }
    },
    "credibility_assessment": {
      "1": {
        "description": "Confirmed by other sources",
        "criteria": ["multiple independent corroboration", "consistent with known facts", "verified information"],
        "confidence_multiplier": 1.2
      },
      "2": {
        "description": "Probably true",
        "criteria": ["logical and consistent", "fits known patterns", "partial corroboration", "plausible"],
        "confidence_multiplier": 1.0
      },
      "3": {
        "description": "Possibly true",
        "criteria": ["reasonable but unconfirmed", "cannot be corroborated", "neither confirmed nor denied"],
        "confidence_multiplier": 0.8
      },
      "4": {
        "description": "Doubtful",
        "criteria": ["questionable logic", "inconsistent with facts", "contradictory elements", "suspicious"],
        "confidence_multiplier": 0.5
      },
      "5": {
        "description": "Improbable",
        "criteria": ["highly unlikely", "contradicts known facts", "illogical", "probable fabrication"],
        "confidence_multiplier": 0.3
      },
      "6": {
        "description": "Truth cannot be judged",
        "criteria": ["insufficient information", "no corroboration possible", "ambiguous", "indeterminate"],
        "confidence_multiplier": 0.6
      }
    }
  },
